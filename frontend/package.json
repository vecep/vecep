{
  "name": "frontend",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@emotion/react": "^11.7.1",
    "@emotion/styled": "^11.6.0",
    "@mui/icons-material": "^5.2.5",
    "@mui/material": "^5.2.7",
    "@testing-library/jest-dom": "^5.16.4",
    "@testing-library/react": "^12.1.2",
    "@testing-library/user-event": "^14.3.0",
    "axios": "^0.24.0",
    "cloudinary-react": "^1.7.2",
    "formik": "^2.2.9",
    "hamburger-react": "^2.4.1",
    "katex": "^0.16.0",
    "lodash": "^4.17.21",
    "mhchemparser": "^4.1.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-latex-next": "^2.1.0",
    "react-router-dom": "^6.2.1",
    "react-scripts": "5.0.0",
    "styled-components": "^5.3.3",
    "web-vitals": "^2.1.3",
    "yup": "^0.32.11"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "eject": "react-scripts eject",
    "test": "jest --config jest.config.json --no-cache",
    "test:noWatch": "npm run test -- --watchAll=false",
    "test:coverage": "npm run test:noWatch -- --coverage --testResultsProcessor jest-sonar-reporter",
    "lint:report": "eslint src/ -f json -o reports/eslint-report.json",
    "sonar:scan": "npm run test:coverage && npm run lint:report && node run-sonar.js"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "eslint": "^8.6.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-jest-dom": "^4.0.2",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.28.0",
    "eslint-plugin-testing-library": "^5.6.0",
    "jest-environment-jsdom": "^28.1.3",
    "jest-sonar-reporter": "^2.0.0",
    "prettier": "^2.5.1",
    "sonarqube-scanner": "^2.8.1"
  },
  "jestSonar": {
    "reportPath": "reports",
    "reportFile": "test-report.xml",
    "indent": 4
  }
}
