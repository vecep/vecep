{
    "env": {
        "es2021": true,
        "node": true
    },
    "extends": ["prettier", "eslint:recommended"],
    "parserOptions": {
        "sourceType": "module"
    },
    "plugins": ["prettier"],
    "rules": {
        "prettier/prettier": "error",
        "no-unused-vars": ["error", { "argsIgnorePattern": "next" }]
    }
}
